#include	"op.h"

int	is_label(char *param)
{
  int	i;
  int	check;

  check = 0;
  i = 0;
  while (param[i])
    {
      if (param[i] == LABEL_CHAR)
	check = 1;
      i++;
    }
  return (check);
}
